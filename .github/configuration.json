    "sort": "ASC",
    "template": "${{CHANGELOG}}[Full Changelog](https://github.com/${{OWNER}}/${{REPO}}/compare/${{FROM_TAG}}...${{TO_TAG}})\n",
    "pr_template": "- ${{TITLE}}\n",
    "empty_template": "- no changes",
    "label_extractor": [
      {
        "pattern": "^(feat|feature|fix|bugfix|refactor|docs|test|style|chore)(.*)",
        "target": "$1"
      }
    ],
    "duplicate_filter": {
      "pattern": ".+",
      "on_property": "title",
      "method": "match"
    },
    "transformers": [
      {
        "pattern": "^(feat|feature|fix|bugfix|refactor|docs|test|style|chore)(.*)",
        "target": "$2"
      }
    ]
}